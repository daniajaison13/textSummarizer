      "source": [
        "def create_model():\n",
        "  # # create model\n",
        "  input_layer = Input(shape=(len(count.get_feature_names_out()), ), name='token_in')\n",
        "  x = Dense(len(count.get_feature_names_out()), activation='relu')(input_layer)\n",
        "  x1 = Dropout(0.2)(x)\n",
        "  x2 = Dense(len(count.get_feature_names_out()) // 2, activation='relu')(x1)\n",
        "  x3 = Dropout(0.2)(x2)\n",
        "  x4 = Dense(len(count.get_feature_names_out()) // 4, activation='relu')(x3)\n",
        "  \n",
        "  y = Dense(6, kernel_initializer='uniform', activation='sigmoid', use_bias = False)(x4)\n",
        "  \n",
        "  y_true = Input( shape=(1,), name='y_true' )\n",
        "  f_scores = Input(shape=(6, ), name='f_scores')\n",
        "  temp_model = Model(inputs = [input_layer, f_scores, y_true], outputs = y)\n",
        "  \n",
        "  model.add_loss( lossFunction( y_true, y,  f_scores) ) \n",
        "  model.compile(loss=tf.keras.losses.CategoricalCrossentropy(), optimizer='sgd')\n",
        "  return temp_model\n",
        "\n",
        "Kmodel = KerasClassifier(build_fn=create_model, verbose = 0)\n",
        "\n",
        "# define the grid search parameters\n",
        "batch_size = [10,40,60]\n",
        "epochs = [10, 50, 100]\n",
        "# optimizer = ['SGD', 'RMSprop', 'Adam']\n",
        "# activation = ['softmax', 'relu', 'tanh', 'sigmoid']\n",
        "param_grid = dict(batch_size=batch_size, epochs=epochs)\n",
        "\n",
        "#implement grid_search\n",
        "grid = GridSearchCV(estimator=Kmodel, param_grid=param_grid, n_jobs=-1, cv=3)\n",
        "grid_result = grid.fit(X_train[count.get_feature_names_out()],y_train)\n",
        "\n",
        "# summarize results\n",
        "print(\"Best: %f using %s\" % (grid_result.best_score_, grid_result.best_params_))\n",
        "means = grid_result.cv_results_['mean_test_score']\n",
        "stds = grid_result.cv_results_['std_test_score']\n",
        "params = grid_result.cv_results_['params']\n",
        "for mean, stdev, param in zip(means, stds, params):\n",
        "    print(\"%f (%f) with: %r\" % (mean, stdev, param))"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "weights_grid = model.predict([X_test[count.get_feature_names_out()], \n",
        "                           X_test[[\"F1\", \"F2\", \"F3\", \"F5\", \"F6\", \"F7\"]], \n",
        "                           y_test])\n",
        "y_true_grid = []\n",
        "y_pred_grid = []\n",
        "i = 0\n",
        "for index, rows in X_test.iterrows():\n",
        "  fscores = X_test[[\"F1\", \"F2\", \"F3\", \"F5\", \"F6\", \"F7\"]].loc[[index]].to_numpy()\n",
        "  value = np.sum(weights_grid[i] * fscores) / sum(weights_grid[i])\n",
        "  y_true_grid.append(y_test.loc[[index]].values[0])\n",
        "  y_pred_grid.append(round(value))\n",
        "  i+=1\n",
        "\n",
        "classification_report_grid = metrics.classification_report(y_true_grid, y_pred_grid)\n",
        "print(classification_report_grid)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
