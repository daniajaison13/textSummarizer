      "outputs": [],
      "source": [
        "data = []\n",
        "for i in range(len(documents)):\n",
        "  f1 = F1_Scores(documents[i])\n",
        "  f2 = F2_Scores(documents[i])\n",
        "  f3 = F3_Scores(documents[i])\n",
        "  f5 = F5_Scores(documents[i])\n",
        "  f6 = F6_Scores(documents[i])\n",
        "  f7 = F7_Scores(documents[i])\n",
        "  for j in range(len(documents[i])):\n",
        "    row = []\n",
        "    if j == 0:\n",
        "      row.append(i)\n",
        "      row.append(j)\n",
        "      row.append(0)\n",
        "      row.append(0)\n",
        "      row.append(0)\n",
        "      row.append(0)\n",
        "      row.append(0)\n",
        "      row.append(0)\n",
        "\n",
        "    else:\n",
        "      row.append(i)\n",
        "      row.append(j)\n",
        "      row.append(f1[j-1])\n",
        "      row.append(f2[j-1])\n",
        "      row.append(f3[j-1])\n",
        "      row.append(f5[j-1])\n",
        "      row.append(f6[j-1])\n",
        "      row.append(f7[j-1])\n",
        "    if(documents[i][j] in summaries[i]):\n",
        "      row.append(1)\n",
        "    else :\n",
        "      row.append(0)\n",
        "    data.append(row)\n",
        "df = pd.DataFrame(data, columns=[\"DocumentNo\", \"Sentence\", \"F1\", \"F2\", \"F3\", \"F5\", \"F6\", \"F7\", \"Target\"])\n",
        "    "
      ]
