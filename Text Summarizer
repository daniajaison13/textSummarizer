        "from nltk.stem.porter import PorterStemmer\n",
        "import re\n",
        "from nltk.stem import WordNetLemmatizer\n",
        "from nltk.stem.snowball import SnowballStemmer\n",
        "nltk.download('wordnet')\n",
        "nltk.download('omw-1.4')\n",
        "lemmatizer = WordNetLemmatizer()\n",
        "snow_stemmer = SnowballStemmer(language='english')"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "si-lLdA0w40z"
      },
      "outputs": [],
      "source": [
        "def stopWordRemoval(sentences, stop_words = set()):\n",
        "  for i in range(len(sentences)):\n",
        "    word_tokens = word_tokenize(sentences[i])\n",
        "    filtered_sentence = [w for w in word_tokens if not w.lower() in stop_words]\n",
        "    sentences[i] = ' '.join(filtered_sentence) + '.'\n",
        "  return sentences"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "K1u-O3H6AohM"
      },
      "outputs": [],
      "source": [
        "def sentences_lemmatization(sentences):\n",
        "  lemmatized_stentences = []\n",
        "  for sentence in sentences:\n",
        "    words = word_tokenize(sentence)\n",
        "    words = [lemmatizer.lemmatize(word) for word in words]\n",
        "    lemmatized_stentences.append(' '.join(words))\n",
        "  return lemmatized_stentences"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "9NKtzGJEAvWw"
      },
      "outputs": [],
      "source": [
        "def sentences_stemming(sentences):\n",
        "  stemmed_sentences = []\n",
        "  porter_stemmer = PorterStemmer()\n",
        "  for i in range(len(sentences)):\n",
        "    words = re.sub(r\"[^A-Za-z0-9\\-]\", \" \", sentences[i]).lower().split()\n",
        "    words = [porter_stemmer.stem(word) for word in words]\n",
        "    stemmed_sentences.append(' '.join(words) + '.')\n",
        "  return stemmed_sentences"
      ]
    },
