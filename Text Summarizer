        "id": "RYGffu9-QwOo"
      },
      "outputs": [],
      "source": [
        "def remove_punctuation(document):\n",
        "  sent=[]\n",
        "  for sen in document:\n",
        "    sentence = sen.translate(str.maketrans('','', string.punctuation))\n",
        "    sent.append(sentence)\n",
        "  document = '. '.join(sent)\n",
        "  document = sent_tokenize(str(document))\n",
        "  return document"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "WW0SNli-Qw2Y"
      },
      "outputs": [],
      "source": [
        "def strip_text(files):\n",
        "  new_documents =[]\n",
        "  for doc in files:\n",
        "    document = remove_punctuation(doc)\n",
        "    new_documents.append(document)\n",
        "  return new_documents"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "LY7j7Q1JEpY0"
      },
      "outputs": [],
      "source": [
        "summaries_formatted = strip_text(summaries)\n",
        "documents_formatted = strip_text(documents)"
      ]
    },
