        "Creating and testing the neural network that gives weights for the 8 features"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "shcfTh9sFQii"
      },
      "outputs": [],
      "source": [
        "input_layer_b3 = Input(shape=(len(count.get_feature_names_out()), ), name='token_in')\n",
        "x_b3 = Dense(len(count.get_feature_names_out()), activation='relu')(input_layer_b3)\n",
        "x1_b3 = Dropout(0.2)(x_b3)\n",
        "x2_b3 = Dense(len(count.get_feature_names_out()) // 2, activation='relu')(x1_b3)\n",
        "x3_b3 = Dropout(0.2)(x2_b3)\n",
        "x4_b3 = Dense(len(count.get_feature_names_out()) // 4, activation='relu')(x3_b3)\n",
        "\n",
        "y_b3 = Dense(8, kernel_initializer='uniform', activation='sigmoid', use_bias = False)(x4_b3)\n",
        "\n",
        "y_true_b3 = Input( shape=(1,), name='y_true_b3' )\n",
        "f_scores_b3 = Input(shape=(8, ), name='f_scores_b3')\n",
        "model_b3 = Model(inputs = [input_layer_b3, f_scores_b3, y_true_b3], outputs = y_b3)\n",
        "\n",
        "model_b3.add_loss( lossFunction( y_true_b3, y_b3,  f_scores_b3) ) \n"
      ]
    },
