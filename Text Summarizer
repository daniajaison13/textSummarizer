      "source": [
        "def displayOutputs(outputs):\n",
        "  W1 = outputs[:, 0]\n",
        "  W2 = outputs[:, 1]\n",
        "  W3 = outputs[:, 2]\n",
        "  W5 = outputs[:, 3]\n",
        "  W6 = outputs[:, 4]\n",
        "  W7 = outputs[:, 5]\n",
        "  print(W1[-1])\n",
        "  print(W2[-1])\n",
        "  print(W3[-1])\n",
        "  print(W5[-1])\n",
        "  print(W6[-1])\n",
        "  print(W7[-1])\n",
        "  plt.plot(W1, label='W1', color = 'violet')\n",
        "  plt.plot(W2, label='W2', color = 'yellow') #\n",
        "  plt.plot(W3, label='W3', color = 'blue')\n",
        "  plt.plot(W5, label='W4', color = 'green') #\n",
        "  plt.plot(W6, label='W5', color = 'orange')\n",
        "  plt.plot(W7, label='W6', color = 'red')"
      ]
