        "y_true = []\n",
        "y_pred = []\n",
        "i = 0\n",
        "for index, rows in X_test.iterrows():\n",
        "  fscores = X_test[[\"F1\", \"F2\", \"F3\", \"F5\", \"F6\", \"F7\"]].loc[[index]].to_numpy()\n",
        "  value = np.sum(weights[i] * fscores) / sum(weights[i])\n",
        "  y_true.append(y_test.loc[[index]].values[0])\n",
        "  y_pred.append(round(value))\n",
        "  i+=1"
      ]
