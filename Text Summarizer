        "def indexToTrack(index):\n",
        "  inputs = [concatedDf.loc[[index]][count.get_feature_names_out()], \n",
        "                      concatedDf.loc[[index]][[\"F1\", \"F2\", \"F3\", \"F5\", \"F6\", \"F7\"]], \n",
        "                      concatedDf.loc[[index]]['Target']]\n",
        "  values_over_epochs = []\n",
        "  # define your custom callback for prediction\n",
        "  class PredictionCallback(Callback):    \n",
        "    def on_epoch_end(self, epoch, logs={}):\n",
        "      print()\n",
        "      # print(inputs)\n",
        "      y_pred = self.model.predict(inputs)\n",
        "      values_over_epochs.append(y_pred[0])\n",
        "  model.compile(loss=None, optimizer='adam' )\n",
        "  history = model.fit(inputs, epochs = 100, callbacks=[PredictionCallback()])\n",
        "  return np.array(values_over_epochs)"
      ]
    },
