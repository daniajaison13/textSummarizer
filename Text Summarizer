      "execution_count": null,
      "metadata": {
        "id": "rj8R84l0xCU9"
      },
      "outputs": [],
      "source": [
        "def getBestSentenceForSummaryWithOptions(file_number, stop_words, shouldLemmatize, shouldStem):\n",
        "  document = documents[file_number - 1]\n",
        "  print(document)\n",
        "  sentences = sent_tokenize(str(document))\n",
        "  sent=[]\n",
        "  for sen in sentences:\n",
        "    sen = re.sub(r'[^\\w\\s]','', sen)\n",
        "    sent.append(sen)\n",
        "  sentences = sent\n",
        "  original_sentence = sentences.copy()\n",
        "  if stop_words:\n",
        "    sentences = stopWordRemoval(sentences, stop_words)\n",
        "  if shouldLemmatize:\n",
        "    sentences = sentences_lemmatization(sentences)\n",
        "  if shouldStem:\n",
        "    sentences = sentences_stemming(sentences)\n",
        "  f1 = F1_Scores(sentences)\n",
        "  f2 = F2_Scores(sentences)\n",
        "  f3 = F3_Scores(sentences)\n",
        "  f5 = F5_Scores(sentences)\n",
        "  f6 = F6_Scores(sentences)\n",
        "  f7 = F7_Scores(sentences)\n",
        "  weights = [8, 8, 3, 5, 8, 8]\n",
        "  scores = []\n",
        "  max_score = 0\n",
        "  best_sentence = ''\n",
        "  for i in range(len(sentences) - 1):\n",
        "    score = weights[0] * f1[i] + weights[1] * f2[i] + weights[2] * f3[i] + weights[3] * f5[i] + weights[4] * f6[i] + weights[5] * f7[i]\n",
        "    scores.append(score / sum(weights))\n",
        "    if score > max_score:\n",
        "      max_score = score\n",
        "      best_sentence_index = i\n",
        "  print(original_sentence[best_sentence_index + 1])\n",
        "  print('Best Sentence Score: ', scores[best_sentence_index])\n",
        "  print('Median Sentence Score: ', statistics.median(scores))\n",
        "  return scores"
      ]
    },
