        "y_pred_b3 = []\n",
        "i = 0\n",
        "for index, rows in X_test_b3.iterrows():\n",
        "  fscores_b3 = X_test_b3[[\"F1\", \"F2\", \"F3\", \"F5\", \"F6\", \"F7\", \"F8\", \"F9\"]].loc[[index]].to_numpy()\n",
        "  value_b3 = np.sum(weights_b3[i] * fscores_b3) / sum(weights_b3[i])\n",
        "  y_true_b3.append(y_test_b3.loc[[index]].values[0])\n",
        "  y_pred_b3.append(round(value))\n",
        "  i+=1"
