        "summaries_corpus = []\n",
        "summaries_dir = Path('/content/drive/MyDrive/sharedata/BBC News Summary/Summaries/sport')\n",
        "for file_path in summaries_dir.files('*.txt'):\n",
        "    with file_path.open(mode='rt', encoding='utf-8') as fp:\n",
        "      if file_path.name in files_to_skip: continue\n",
        "      try:\n",
        "        lines = fp.read().splitlines()\n",
        "        summaries_corpus.append((file_path.name, [i for i in lines if i]))\n",
        "      except:\n",
        "        print('Error parsing the following file: ', file_path.name)\n",
        "        files_to_skip.append(file_path.name)\n",
        "summaries_corpus.sort(key = lambda x: x[0])\n",
        "summaries_corpus = [item[1] for item in summaries_corpus]\n",
        "summaries = []\n",
        "for x in summaries_corpus:\n",
        "  common = ''\n",
        "  for i, sentence in enumerate(x):\n",
        "    if i == 0: \n",
        "      common += sentence + '.\\n'\n",
        "    else: common += sentence + '\\n'\n",
        "  summaries.append(common)"
      ]
    },
