        "from scipy.spatial import distance"
      ],
      "metadata": {
        "id": "9_VKXzSDpV8n"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def similarityScorePerCorpus(documents, topic_sent):\n",
        "\n",
        "  y_pred =[]\n",
        "  \n",
        "\n",
        "  for index in range(len(documents)):\n",
        "    art_df = pd.DataFrame(documents[index], columns=['sentences'])\n",
        "    art_df['Embeddings']=None \n",
        "    art_df['SimilarityScore']=None\n",
        "\n",
        "    topic_embed = embed([topic_sent[index]])\n",
        "\n",
        "    for i in range(len(art_df)):\n",
        "      if i == 0:\n",
        "        art_df['SimilarityScore'][i] = 0\n",
        "        continue\n",
        "      art_df['Embeddings'][i] = embed([str(documents[index][i])])\n",
        "      #print(art_df)\n",
        "      art_df['SimilarityScore'][i] = (1 - distance.cosine(topic_embed, art_df['Embeddings'][i]))\n",
        "    TFHub = art_df[['sentences','SimilarityScore']].sort_values('SimilarityScore', ascending = False)\n",
        "    TFHub.reset_index(inplace = True)\n",
        "    #print(TFHub)\n",
        "    #print(type(TFHub))\n",
        "    #print(TFHub['sentences'][0])\n",
        "    sim_threshold = 0.25\n",
        "    for i in range(len(art_df)):\n",
        "      \n",
        "      if(art_df['SimilarityScore'][i] > sim_threshold):\n",
        "        y_pred.append(1)\n",
        "      else:\n",
        "        y_pred.append(0)\n",
        "  \n",
        "  return y_pred\n",
        "  \n",
        "\n"
      ],
      "metadata": {
        "id": "iY9qvI7U7prg"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "y_pred = similarityScorePerCorpus(documents,topic_sent)\n",
        "\n"
      ],
      "metadata": {
        "id": "ZIRTkpSGy5z1"
      },
      "execution_count": null,
      "outputs": []
    },
