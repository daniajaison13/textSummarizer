        "      row.append(0)\n",
        "\n",
        "    else:\n",
        "      row.append(i)\n",
        "      row.append(j)\n",
        "      row.append(f1[j-1])\n",
        "      row.append(f2[j-1])\n",
        "      row.append(f3[j-1])\n",
        "      row.append(f5[j-1])\n",
        "      row.append(f6[j-1])\n",
