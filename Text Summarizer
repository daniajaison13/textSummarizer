        "id": "UInBpwhAnp5x"
      },
      "outputs": [],
      "source": [
        "documents = []\n",
        "for x in corpus:\n",
        "  common = ''\n",
        "  for i, sentence in enumerate(x):\n",
        "    if i == 0: \n",
        "      common += sentence + '.\\n'\n",
        "    else: common += sentence + '\\n'\n",
        "  documents.append(common)"
      ]
    },
