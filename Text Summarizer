        }
      ],
      "source": [
        "corpus = []\n",
        "corpus_dir = Path('/content/drive/MyDrive/sharedata/BBC News Summary/News Articles/sport')\n",
        "\n",
        "for file_path in corpus_dir.files('*.txt'):\n",
        "    with file_path.open(mode='rt', encoding='utf-8') as fp:\n",
        "      try:\n",
        "        lines = fp.read().splitlines()\n",
        "        corpus.append((file_path.name, [i for i in lines if i]))\n",
        "      except:\n",
        "        print('Error parsing the following file: ', file_path.name)\n",
        "        corpus.append((file_path.name, []))"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "MxwO2va45WBG"
      },
      "outputs": [],
      "source": [
        "corpus.sort(key = lambda x: x[0])\n",
        "corpus = [item[1] for item in corpus]"
      ]
    },
